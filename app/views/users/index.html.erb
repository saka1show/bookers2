<%= render '/books/sidebar', book: @book %>
<div class="col-md-8 offset-md-1">
  <h1>Users</h1>

  <% @users.each do |user| %>

  <p>image</p>
  <%= image_tag(user.profile_image.presence || 'no_image.jpg', size:'100x100') %>

  <p>name</p>
  <span><%= user.name %></span>

  <p><%= link_to "Show", user_path(user) %></p>

  <% end %>
</div>